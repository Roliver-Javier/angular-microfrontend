{
  "$schema": "./node_modules/@angular-devkit/core/src/workspace/workspace-schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shell": {
      "root": "",
      "projectType": "application",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/shell",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "./tsconfig.app.json",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "src/img",
                "output": "/img"
              }
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/paper-dashboard.css",
              "src/assets/css/demo.css",
              "src/assets/css/themify-icons.css"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "shell:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "shell:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shell:build"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              {
                "input": "styles.css"
              }
            ],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "shell-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "shell:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "client-a": {
      "root": "projects/client-a/",
      "projectType": "application",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/shell/assets/micro-frontends/client-a/browser",
            "index": "projects/client-a/src/index.html",
            "main": "projects/client-a/src/main.ts",
            "polyfills": "projects/client-a/src/polyfills.ts",
            "tsConfig": "projects/client-a/tsconfig.app.json",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "projects/client-a/src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "projects/client-a/src/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              "projects/client-a/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "debug": {
              "fileReplacements": [
                {
                  "replace": "projects/client-a/src/environments/environment.ts",
                  "with": "projects/client-a/src/environments/environment.prod.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/client-a/src/environments/environment.ts",
                  "with": "projects/client-a/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "client-a:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "client-a:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client-a:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/client-a/src/test.ts",
            "polyfills": "projects/client-a/src/polyfills.ts",
            "tsConfig": "projects/client-a/tsconfig.spec.json",
            "karmaConfig": "projects/client-a/karma.conf.js",
            "styles": [
              {
                "input": "projects/client-a/styles.css"
              }
            ],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "projects/client-a/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "projects/client-a/src/assets",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/client-a/tsconfig.app.json",
              "projects/client-a/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/shell/assets/micro-frontends/client-a/server",
            "main": "projects/client-a/src/main.server.ts",
            "tsConfig": "projects/client-a/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/client-a/src/environments/environment.ts",
                  "with": "projects/client-a/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            }
          }
        }
      }
    },
    "client-b": {
      "root": "projects/client-b/",
      "projectType": "application",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:build",
          "options": {
            "outputPath": "dist/client-b",
            "index": "projects/client-b/src/index.html",
            "main": "projects/client-b/src/main.ts",
            "polyfills": "projects/client-b/src/polyfills.ts",
            "tsConfig": "projects/client-b/tsconfig.app.json",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "projects/client-b/src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "projects/client-b/src/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              "projects/client-b/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/client-b/src/environments/environment.ts",
                  "with": "projects/client-b/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "client-b:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "client-b:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client-b:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/client-b/src/test.ts",
            "polyfills": "projects/client-b/src/polyfills.ts",
            "tsConfig": "projects/client-b/tsconfig.spec.json",
            "karmaConfig": "projects/client-b/karma.conf.js",
            "styles": [
              {
                "input": "projects/client-b/styles.css"
              }
            ],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "projects/client-b/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "projects/client-b/src/assets",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/client-b/tsconfig.app.json",
              "projects/client-b/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "client-a-e2e": {
      "root": "projects/client-a-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/client-a-e2e/protractor.conf.js",
            "devServerTarget": "client-a:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/client-a-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "css"
    }
  }
}
