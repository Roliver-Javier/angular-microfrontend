{
  "name": "dashboard-elments",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project dashboard-elments -o",
    "static-server": "ng run static-server",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngx-build-plus:copy-assets": "node copy-wc-polyfill.js && node copy-bundles.js && node projects/client-a/copy-bundles.js && node projects/client-b/copy-bundles.js",
    "build:clients": "npm run build:client-a && npm run build:client-b",
    "build:client-a": "ng build --prod --single-bundle --project client-a --output-hashing none --vendor-chunk false --output-path src/assets/micro-frontends/client-a",
    "build:client-b": "ng build --prod --single-bundle --project client-b --output-hashing none --vendor-chunk false --output-path src/assets/micro-frontends/client-b",
    "build:externals": "ng build --extra-webpack-config webpack.externals.js --prod --single-bundle",
    "build:dashboard-elments:externals": "ng build --extra-webpack-config webpack.externals.js --prod --project dashboard-elments --single-bundle"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/elements": "^8.2.14",
    "@angular/forms": "~8.2.11",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "@swimlane/ngx-charts": "^8.1.0",
    "@webcomponents/custom-elements": "^1.3.2",
    "bootstrap": "^3.3.7",
    "copy": "^0.3.2",
    "document-register-element": "^1.7.2",
    "ngx-build-plus": "^9.0.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.12",
    "@angular/cli": "^8.3.23",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
